{

  "name": "Prevent Reload",
  "version": "0",
  "description": "Show browser width",
  "background": {
                 "persistent": false,
                 "scripts": ["background.js"]
                 },
  "permissions": [
  "activeTab",
  "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    }
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js" ],
    "css": ["customStyles.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
